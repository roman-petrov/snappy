{
  "absolute": true,
  "ignore": [
    ".github",
    ".jscpd",
    "**/dist/**",
    "**/*.html",
    "**/*.test.ts",
    "**/package.json",
    "**/node_modules",
    "**/packages/*/src/generated"
  ],
  "ignorePattern": ["import.*from.*"],
  "gitignore": true,
  "mode": "strict",
  "output": ".jscpd",
  "reporters": ["console", "html"],
  "threshold": 0
}
